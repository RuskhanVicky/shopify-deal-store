        "default": "style-1",
        "options": [
          { "value": "style-1", "label": "Style 1" },
          { "value": "style-2", "label": "Style 2" }
        ]
      },
      {
        "type": "number",
        "id": "p_dot",
        "label": "Position bottom navigation",
