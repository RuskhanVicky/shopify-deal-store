      {
        "type": "checkbox",
        "id": "arrows",
        "label": "Enable arrows",
        "default": false
      },
      {
        "type": "header",
        "content": "Navigation"
      },
      {
        "type": "checkbox",
        "id": "dots",
        "label": "Enable navigation",
        "default": true
      },
      {
        "type": "select",
        "id": "s_dot",
