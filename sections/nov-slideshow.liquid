{% assign sst = section.settings %}
{%- if settings.lang_iso_rtl != blank -%}
  {% assign list_iso_rtl = settings.lang_iso_rtl | downcase %}
  {% if list_iso_rtl contains request.locale.iso_code %}
    {% assign iso_rtl = true %}
  {% else %}
    {% assign iso_rtl = false %}
  {% endif %}
{%- endif -%}
<div class="distance{% if sst.class != blank %} {{ sst.class }}{% endif %}{% if sst.animate == true %} wow fadeInUp animated{% endif %}" data-section-id="{{ section.id }}" data-section-type="slideshow-section">
  {% if sst.container == 'container' %}
    <div class="container">
  {% elsif sst.container == 'container-fluid' %}
    <div class="container-fluid">
  {% else %}
    <div class="container-full">
  {% endif %}
    {% if sst.show_banner %}
      <div class="show-banner d-lg-flex">
    {% endif %}
    {% comment %} Slideshow {% endcomment %}
    <div class="slideshow position-relative">
      {% if section.blocks.size > 0 %}
        <div class="main-slider dot-{{ sst.s_dot }}" data-autoplay="{{ sst.autoplay }}" data-speed="{{ sst.autoplay_speed }}" data-arrows="{{ sst.arrows }}" data-dots="{{ sst.dots }}"{% if sst.loading_bar %} data-loading-bar{% endif %} data-zoom="{{ sst.zoom }}">
          {% for block in section.blocks %}
            {% assign bst = block.settings %}
            <div class="item">
              {% if bst.image != blank %}
                <div class="slide-image s-{{ forloop.index }}{% if forloop.first %} first-scale{% endif %}">
                  {% assign img_url = bst.image %}
                  <div class="respone_image{% if bst.img_sm != blank %} d-none d-md-block{% endif %}" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                    <img class="image-entity lazyload"
                      src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 0.6'%3E%3C/svg%3E"
                      data-src="{{ img_url | img_url: '360x' }}"
                      data-srcset="{{ img_url | img_url: '768x' }} 768w,{{ img_url | img_url: '900x' }} 900w,{{ img_url | img_url: '1080x' }} 1080w,{{ img_url | img_url: '1296x' }} 1296w,{{ img_url | img_url: '1512x' }} 1512w,{{ img_url | img_url: '1728x' }} 1728w, {{ img_url | img_url: '2048x' }} 2048w" data-sizes="auto" alt="{{ img_url.alt }}">
                  </div>
                  {% if bst.img_sm != blank %}
                    {% assign img_url = bst.img_sm %}
                    <div class="respone_image d-md-none" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                      <img class="image-entity lazyload"
                        data-src="{{ img_url | img_url: '360x' }}"
                        data-srcset="{{ img_url | img_url: '380x' }} 380w,{{ img_url | img_url: '580x' }} 580w, {{ img_url | img_url: '767x' }} 767w" data-sizes="auto" alt="{{ img_url.alt }}">
                    </div>
                  {% endif %}
                </div>
              {% else %} 
                {% capture current %}{% cycle 1, 2, 3 %}{% endcapture %}
                {{ 'hero-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
              {% endif %}
              {% unless bst.img == blank %}
                <div class="caption-img-element-{{ forloop.index }}{% if bst.img_animate %} img_animate{% endif %} d-inline-block position-absolute">
                  <img data-src="{{ bst.img | img_url: '600x' }}" class="lazyload layer w-100" data-depth="0.4">
                </div>
              {% endunless %}
              {% if bst.cap_content == 'container' %}
                <div class="cap_content container">
              {% endif %}
              <div class="content position-absolute content-{{ forloop.index }} text-{{ bst.text_align }}">
                {% unless bst.caption_1 == blank %}
                  <div class="caption-animate caption-1{% if bst.ff1 == 'heading' %} f_pr{% endif %}" data-animate="{{ bst.cap_animate_1 }} animated" style="color: {{ bst.color_cap1 }};">{{ bst.caption_1 | nofilter }}</div>
                {% endunless %}
                {% unless bst.caption_2 == blank %}
                  <div class="caption-animate caption-2{% if bst.ff2 == 'heading' %} f_pr{% endif %}" data-animate="{{ bst.cap_animate_2 }} animated" style="color: {{ bst.color_cap2 }};">{{ bst.caption_2 | nofilter }}</div>
                {% endunless %}
                {% unless bst.caption_3 == blank %}
                  <div class="caption-animate caption-3{% if bst.ff3 == 'heading' %} f_pr{% endif %}" data-animate="{{ bst.cap_animate_3 }} animated" style="color: {{ bst.color_cap3 }};">{{ bst.caption_3 | nofilter }}</div>
                {% endunless %}
                {% unless bst.caption_4 == blank %}
                  <div class="caption-animate caption-4{% if bst.ff4 == 'heading' %} f_pr{% endif %}" data-animate="{{ bst.cap_animate_4 }} animated" style="color: {{ bst.color_cap4 }};">{{ bst.caption_4 | nofilter }}</div>
                {% endunless %}
                {% if bst.caption_btn != blank or bst.slider_link2 != blank %}
                  {% if bst.caption_btn != blank %}
                    <a class="caption-animate btn" href="{{ bst.slider_link }}" style="background-color: {{ bst.b_btn }}; color: {{ bst.c_btn }};">
                      <span class="d-flex align-items-center justify-content-center">
                        {% if bst.caption_btn_i != blank %}<img src="{{ bst.caption_btn_i | img_url: '12x' }}">{% endif %}
                        <span>{{ bst.caption_btn | nofilter }}</span>
                      </span>
                    </a>
                  {% endif %}
                  {% if bst.caption_btn2 != blank %}
                    <a class="caption-animate btn btn-2" href="{{ bst.slider_link2 }}" style="background-color: {{ bst.b_btn }}; color: {{ bst.c_btn }};">
                      <span class="font-700 d-flex align-items-center justify-content-center">
                        {% if bst.caption_btn_i2 != blank %}<img src="{{ bst.caption_btn_i2 | img_url: '12x' }}">{% endif %}
                        <span>{{ bst.caption_btn2 | nofilter }}</span>
                      </span>
                    </a>
                  {% endif %}
                {% endif %}
              </div>
              {% if bst.cap_content == 'container' %}
                </div>
              {% endif %}
            </div> 
          {% endfor %}
        </div>
      {% endif %}
      {% if section.blocks.size == 0 %}
        <div class="placeholder-noblocks">
          {{ 'homepage.onboarding.no_content' | t }}
        </div>
      {% endif %}
    </div>
    {% comment %} End Slideshow {% endcomment %}

    {% if sst.show_banner %}
        <div class="block-banner mt-md-10">
          <div class="banner-content row spacing-10">
            <div class="{% if sst.bn3 != blank %}col-md-6{% else %}col-md-12{% endif %}">
              {% if sst.bn1 != blank %}
                {% if sst.bn_link1 != blank %}
                  <a href="{{ sst.bn_link1 }}" class="nov-sh-image-2 position-relative no-sh-xs d-block{% if sst.bn2 == blank %} h-100{% endif %}">
                {% else %}
                  <div class="nov-sh-image-2 position-relative no-sh-xs{% if sst.bn2 == blank %} h-100{% endif %}">
                {% endif %}
                  {%- render 'nov-aspect-image', image: sst.bn1, aspect: 'auto' -%}
                  {% if sst.bn_t1 != blank %}
                    <div class="bn-title position-absolute bottom-center f_pr white font-700 text-center w-100">{{ sst.bn_t1 }}</div>
                  {% endif %}
                {% if sst.bn_link1 != blank %}
                  </a>
                {% else %}
                  </div>
                {% endif %}
              {% endif %}
              {% if sst.bn2 != blank %}
                {% if sst.bn_link2 != blank %}
                  <a href="{{ sst.bn_link2 }}" class="nov-sh-image-2 position-relative no-sh-xs d-block mt-10">
                {% else %}
                  <div class="nov-sh-image-2 position-relative no-sh-xs mt-10">
                {% endif %}
                  {%- render 'nov-aspect-image', image: sst.bn2, aspect: 'auto' -%}
                  {% if sst.bn_t2 != blank %}
                    <div class="bn-title position-absolute bottom-center f_pr white font-700 text-center w-100">{{ sst.bn_t2 }}</div>
                  {% endif %}
                {% if sst.bn_link2 != blank %}
                  </a>
                {% else %}
                  </div>
                {% endif %}
              {% endif %}
            </div>
            {% if sst.bn3 != blank %}
              <div class="col-md-6 mt-sm-10">
                {% if sst.bn_link3 != blank %}
                  <a href="{{ sst.bn_link3 }}" class="nov-sh-image-2 position-relative no-sh-xs d-block h-100">
                {% else %}
                  <div class="nov-sh-image-2 position-relative no-sh-xs h-100">
                {% endif %}
                  {%- render 'nov-aspect-image', image: sst.bn3, aspect: 'auto' -%}
                  {% if sst.bn_t3 != blank %}
                    <div class="bn-title position-absolute bottom-center f_pr white font-700 text-center w-100">{{ sst.bn_t3 }}</div>
                  {% endif %}
                {% if sst.bn_link3 != blank %}
                  </a>
                {% else %}
                  </div>
                {% endif %}
              </div>
            {% endif %}
          </div>
        </div>
      </div>
    {% endif %}
  </div>
</div>

{% schema %}
  {
