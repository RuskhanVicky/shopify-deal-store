<style>
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top }}px;
    padding-bottom: {{ sst.distance_bottom }}px;
  }
  #shopify-section-{{ section.id }} .timer:before, #shopify-section-{{ section.id }} .dot-style-2 .slick-active:before {
    animation-duration: {{ sst.autoplay_speed | divided_by: 1000 | append: 's' }};
  }
  #shopify-section-{{ section.id }} .timer:before {
    background-color: rgba({{ sst.bg_load | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bg_load_opacity }}%);
  }
  #shopify-section-{{ section.id }} .dot-style-1 .slick-dots li button,
  #shopify-section-{{ section.id }} .dot-style-1 .slick-dots li:before {
    background-color: {{ sst.c_dot }};
  }
  #shopify-section-{{ section.id }} .dot-style-1 .slick-dots li.slick-active button,
  #shopify-section-{{ section.id }} .dot-style-1 .slick-dots li:hover button {
    background-color: transparent;
    box-shadow: 0 0 0 2px {{ sst.c_dot }};
  }
  #shopify-section-{{ section.id }} .dot-style-2 .slick-dots li {
    background-color: {{ sst.bg_dot2 }};
  }
  #shopify-section-{{ section.id }} .dot-style-2 .slick-dots li:before {
    background-color: {{ sst.c_dot }};
  }
  #shopify-section-{{ section.id }} .main-slider {
    border-radius: {{ sst.border_radius }}px;
    overflow: hidden;
    transform: translateZ(0);
  }
  #shopify-section-{{ section.id }} .nov-sh-image-2 {
    border-radius: {{ sst.br_banner }}px;
  }
  #shopify-section-{{ section.id }} .slick-dots {
    bottom: min({{ sst.p_dot | append: '%' }}, 25px);
  }
  @media(min-width:  768px) {
    #shopify-section-{{ section.id }} .slick-dots {
      bottom: {{ sst.p_dot | append: '%' }};
    }
  }
  {% for block in section.blocks %}
    {% assign bst = block.settings %}
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} {
      {% if settings.enable_rtl and iso_rtl %}{{ bst.position | downcase | replace: 'left', 'right' }}{% else %}{{ bst.position }}{% endif %};
    }
    #shopify-section-{{ section.id }} .caption-img-element-{{ forloop.index }} {
      {% if settings.enable_rtl and iso_rtl %}{{ bst.position_img | downcase | replace: 'left', 'right' }}{% else %}{{ bst.position_img }}{% endif %};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-1 {
      font-size: {{ bst.fz_cap1_xxl }}px;
      font-weight: {{ bst.fw1 }};
      font-style: {{ bst.fs1 }};
      letter-spacing: {{ bst.let1 }}px;
      margin-bottom: {{ bst.mb1 }}px;
      line-height: {{ bst.line_height1 }};
      text-transform: {{ bst.tt1 }};
      -webkit-text-stroke: {{ bst.ts1_w }}px {{ bst.ts1_c }};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-2 {
      font-size: {{ bst.fz_cap2_xxl }}px;
      font-weight: {{ bst.fw2 }};
      font-style: {{ bst.fs2 }};
      letter-spacing: {{ bst.let2 }}px;
      margin-bottom: {{ bst.mb2 }}px;
      line-height: {{ bst.line_height2 }};
      text-transform: {{ bst.tt2 }};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-3 {
      font-size: {{ bst.fz_cap3_xxl }}px;
      font-weight: {{ bst.fw3 }};
      font-style: {{ bst.fs3 }};
      letter-spacing: {{ bst.let3 }}px;
      margin-bottom: {{ bst.mb3 }}px;
      line-height: {{ bst.line_height3 }};
      text-transform: {{ bst.tt3 }};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-4 {
      font-size: {{ bst.fz_cap4_xxl }}px;
      font-weight: {{ bst.fw4 }};
      font-style: {{ bst.fs4 }};
      letter-spacing: {{ bst.let4 }}px;
      margin-bottom: {{ bst.mb4 }}px;
      line-height: {{ bst.line_height4 }};
      text-transform: {{ bst.tt4 }};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .btn:hover {
      color: {{ bst.c_btn_h }} !important;
      background-color: {{ bst.b_btn_h }} !important;
    }
    @media(max-width: 1599px) {
      #shopify-section-{{ section.id }} .caption-img-element-{{ forloop.index }} {
        {% if settings.enable_rtl and iso_rtl %}{{ bst.position_xl_img | downcase | replace: 'left', 'right' }}{% else %}{{ bst.position_xl_img }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} {
        {% if settings.enable_rtl and iso_rtl %}{{ bst.position_xl | downcase | replace: 'left', 'right' }}{% else %}{{ bst.position_xl }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-1 {
        font-size: {{ bst.fz_cap1_xl }}px;
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-2 {
