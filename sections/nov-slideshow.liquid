      {
        "type": "header",
        "content": "Banner"
      },
      {
        "type": "checkbox",
        "id": "show_banner",
        "label": "Show block banner"
      },
      {
        "type": "header",
        "content": "Banner 1"
      },
      {
        "type": "image_picker",
        "id": "bn1",
        "label": "Banner"
      },
      {
        "type": "text",
        "id": "bn_t1",
        "label": "Banner title"
      },
      {
        "type": "url",
        "id": "bn_link1",
        "label": "Banner link"
      },
      {
        "type": "header",
        "content": "Banner 2"
      },
      {
        "type": "image_picker",
        "id": "bn2",
        "label": "Banner"
      },
      {
        "type": "text",
        "id": "bn_t2",
        "label": "Banner title"
      },
      {
        "type": "url",
        "id": "bn_link2",
        "label": "Banner link"
      },
      {
        "type": "header",
        "content": "Banner 3"
      },
      {
        "type": "image_picker",
        "id": "bn3",
        "label": "Banner"
      },
      {
        "type": "text",
        "id": "bn_t3",
        "label": "Banner title"
      },
      {
        "type": "url",
        "id": "bn_link3",
        "label": "Banner link",
        "info": "========================="
      },
      {
        "type": "range",
        "id": "br_banner",
        "min": 0,
        "max": 100,
        "step": 1,
        "label": "Border radius banner",
        "default": 0
      },
      {
        "type":  "header",
        "content": "Design option"
